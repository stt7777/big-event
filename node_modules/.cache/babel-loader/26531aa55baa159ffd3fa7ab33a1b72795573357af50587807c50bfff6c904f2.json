{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapGetters } from 'vuex';\nexport default {\n  name: 'my-layout',\n  computed: {\n    ...mapGetters(['username', 'nickname', 'user_pic'])\n  },\n  methods: {\n    quitFn() {\n      this.$confirm('这就退出了吗, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        // 清除vuex操作，强制跳转登录页面，清除userinfo\n        this.$store.commit('updateToken', '');\n        this.$router.push('/login');\n        this.$store.commit('updateUserInfo', {});\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: '已取消登录'\n        });\n      });\n    },\n    handleOpen(key, keyPath) {\n      console.log(key, keyPath);\n    },\n    handleClose(key, keyPath) {\n      console.log(key, keyPath);\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","name","computed","methods","quitFn","$confirm","confirmButtonText","cancelButtonText","type","then","$store","commit","$router","push","catch","$message","message","handleOpen","key","keyPath","console","log","handleClose"],"sources":["src/views/layout/index.vue"],"sourcesContent":["<template>\r\n  <el-container class=\"main-container\">\r\n    <!-- 头部区域 -->\r\n    <el-header>\r\n      <!-- 左侧的 logo -->\r\n      <img src=\"../../assets/images/logo.png\" alt=\"\" />\r\n      <!-- 右侧的菜单 -->\r\n      <el-menu\r\n        class=\"el-menu-top\"\r\n        mode=\"horizontal\"\r\n        background-color=\"#23262E\"\r\n        text-color=\"#fff\"\r\n        active-text-color=\"#409EFF\"\r\n      >\r\n        <el-submenu index=\"1\">\r\n          <template slot=\"title\">\r\n            <!-- 头像 -->\r\n            <img src=\"../../assets/images/logo.png\" alt=\"\" class=\"avatar\" />\r\n            <span>个人中心</span>\r\n          </template>\r\n          <el-menu-item index=\"1-1\"><i class=\"el-icon-s-operation\"></i>基本资料</el-menu-item>\r\n          <el-menu-item index=\"