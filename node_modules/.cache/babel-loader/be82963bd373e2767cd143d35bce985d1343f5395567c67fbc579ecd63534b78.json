{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapGetters } from 'vuex';\nimport { getMenuListAPI } from '@/api/index.js';\nexport default {\n  name: 'my-layout',\n  computed: {\n    ...mapGetters(['username', 'nickname', 'user_pic'])\n  },\n  data() {\n    return {\n      menus: []\n    };\n  },\n  methods: {\n    quitFn() {\n      this.$confirm('这就退出了吗, 是否继续?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        // 清除vuex操作，强制跳转登录页面，清除userinfo\n        this.$store.commit('updateToken', '');\n        this.$router.push('/login');\n        this.$store.commit('updateUserInfo', {});\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: '已取消登录'\n        });\n      });\n    },\n    handleOpen(key, keyPath) {\n      console.log(key, keyPath);\n    },\n    handleClose(key, keyPath) {\n      console.log(key, keyPath);\n    },\n    async getMenuListFn() {\n      const {\n        data: res\n      } = await getMenuListAPI();\n      console.log(res);\n      this.menus = res.data;\n    }\n  },\n  created() {\n    this.getMenuListFn();\n  }\n};","map":{"version":3,"names":["mapGetters","getMenuListAPI","name","computed","data","menus","methods","quitFn","$confirm","confirmButtonText","cancelButtonText","type","then","$store","commit","$router","push","catch","$message","message","handleOpen","key","keyPath","console","log","handleClose","getMenuListFn","res","created"],"sources":["src/views/layout/index.vue"],"sourcesContent":["<template>\r\n  <el-container class=\"main-container\">\r\n    <!-- 头部区域 -->\r\n    <el-header>\r\n      <!-- 左侧的 logo -->\r\n      <img src=\"../../assets/images/logo.png\" alt=\"\" />\r\n      <!-- 右侧的菜单 -->\r\n      <el-menu\r\n        class=\"el-menu-top\"\r\n        mode=\"horizontal\"\r\n        background-color=\"#23262E\"\r\n        text-color=\"#fff\"\r\n        active-text-color=\"#409EFF\"\r\n      >\r\n        <el-submenu index=\"1\">\r\n          <template slot=\"title\">\r\n            <!-- 头像 -->\r\n            <img src=\"../../assets/images/logo.png\" alt=\"\" class=\"avatar\" />\r\n            <span>个人中心</span>\r\n          </template>\r\n          <el-menu-item index=\"1-1\"><i class=\"el-icon-s-operation\"></i>基本资料</el-menu-item>\r\n          <el-menu-item index=\"1-2\"><i class=\"el-icon-camera\"></i>更换头像</el-menu-item>\r\n          <el-menu-item index=\"1-3\"><i class=\"el-icon-key\"></i>重置密码</el-menu-item>\r\n        </el-submenu>\r\n        <el-menu-item index=\"2\"><i class=\"el-icon-switch-button\"  @click=\"quitFn\"></i>退出</el-menu-item>\r\n      </el-menu>\r\n    </el-header>\r\n    <el-container>\r\n      <!-- 侧边栏区域 -->\r\n      <el-aside width=\"200px\">\r\n    <div class=\"user-box\">\r\n        <img :src=\"user_pic\" alt=\"\" v-if=\"user_pic\" />\r\n        <img src=\"../../assets/images/logo.png\" alt=\"\" v-else />\r\n        <span>欢迎 {{ nickname || username }}</span>\r\n    </div>\r\n    <!-- 侧边栏的组件信息 -->\r\n    <el-menu\r\n      default-active=\"/home\"\r\n      class=\"el-menu-vertical-demo\"\r\n      unique-opened\r\n      router\r\n      @open=\"handleOpen\"\r\n      @close=\"handleClose\"\r\n      background-color=\"#23262E\"\r\n      text-color=\"#fff\"\r\n      active-text-color=\"#409EFF\">\r\n        <template v-for=\"item in menus\">\r\n          <el-menu-item v-if=\"!item.children\" :index=\"item.indexPath\" :key=\"item.indexPath\">\r\n                <i :class=\"item.icon\"></i>\r\n                <span slot=\"title\">{{ item.title }}</span>\r\n          </el-menu-item>\r\n\r\n          <el-submenu v-else :index=\"item.indexPath\" :key=\"item.icon\">\r\n                <template slot=\"title\">\r\n                  <i :class=\"item.icon\"></i>\r\n                  <span>{{item.title}}</span>\r\n                </template>\r\n                <el-menu-item v-for=\"obj,index in item.children\" :index=\"obj.indexPath\" :key=\"index\">\r\n                <i :class=\"obj.icon\"></i>\r\n                <span>{{ obj.title  }}</span>\r\n              </el-menu-item>\r\n          </el-submenu>\r\n        </template>\r\n\r\n    </el-menu>\r\n</el-aside>\r\n      <el-container>\r\n        <!-- 页面主体区域 -->\r\n        <el-main>\r\n           <router-view></router-view>\r\n        </el-main>\r\n        <!-- 底部 footer 区域 -->\r\n        <el-footer>© www.itheima.com - 黑马程序员</el-footer>\r\n      </el-container>\r\n    </el-container>\r\n  </el-container>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex'\r\nimport { getMenuListAPI } from '@/api/index.js'\r\nexport default {\r\n  name: 'my-layout',\r\n  computed: {\r\n    ...mapGetters(['username', 'nickname', 'user_pic'])\r\n  },\r\n  data () {\r\n    return {\r\n      menus: []\r\n    }\r\n  },\r\n  methods: {\r\n    quitFn () {\r\n      this.$confirm('这就退出了吗, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        // 清除vuex操作，强制跳转登录页面，清除userinfo\r\n        this.$store.commit('updateToken', '')\r\n        this.$router.push('/login')\r\n        this.$store.commit('updateUserInfo', {})\r\n      }).catch(() => {\r\n        this.$message({\r\n          type: 'info',\r\n          message: '已取消登录'\r\n        })\r\n      })\r\n    },\r\n    handleOpen (key, keyPath) {\r\n      console.log(key, keyPath)\r\n    },\r\n    handleClose (key, keyPath) {\r\n      console.log(key, keyPath)\r\n    },\r\n    async getMenuListFn () {\r\n      const { data: res } = await getMenuListAPI()\r\n      console.log(res)\r\n      this.menus = res.data\r\n    }\r\n  },\r\n  created () {\r\n    this.getMenuListFn()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.main-container {\r\n  height: 100%;\r\n  .el-header,\r\n  .el-aside {\r\n    background-color: #23262e;\r\n  }\r\n  .el-header {\r\n    padding: 0;\r\n    display: flex;\r\n    justify-content: space-between;\r\n  }\r\n  .el-main {\r\n    overflow-y: scroll;\r\n    height: 0;\r\n    background-color: #F2F2F2;\r\n  }\r\n  .el-footer {\r\n    background-color: #eee;\r\n    font-size: 12px;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n  }\r\n}\r\n\r\n.avatar {\r\n  border-radius: 50%;\r\n  width: 35px;\r\n  height: 35px;\r\n  background-color: #fff;\r\n  margin-right: 10px;\r\n  object-fit: cover;\r\n}\r\n.user-box {\r\n  height: 70px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  border-top: 1px solid #000;\r\n  border-bottom: 1px solid #000;\r\n  user-select: none;\r\n  img {\r\n    width: 35px;\r\n    height: 35px;\r\n    border-radius: 50%;\r\n    background-color: #fff;\r\n    margin-right: 15px;\r\n    object-fit: cover;\r\n  }\r\n  span {\r\n    color: white;\r\n    font-size: 12px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";AA+EA,SAAAA,UAAA;AACA,SAAAC,cAAA;AACA;EACAC,IAAA;EACAC,QAAA;IACA,GAAAH,UAAA;EACA;EACAI,KAAA;IACA;MACAC,KAAA;IACA;EACA;EACAC,OAAA;IACAC,OAAA;MACA,KAAAC,QAAA;QACAC,iBAAA;QACAC,gBAAA;QACAC,IAAA;MACA,GAAAC,IAAA;QACA;QACA,KAAAC,MAAA,CAAAC,MAAA;QACA,KAAAC,OAAA,CAAAC,IAAA;QACA,KAAAH,MAAA,CAAAC,MAAA;MACA,GAAAG,KAAA;QACA,KAAAC,QAAA;UACAP,IAAA;UACAQ,OAAA;QACA;MACA;IACA;IACAC,WAAAC,GAAA,EAAAC,OAAA;MACAC,OAAA,CAAAC,GAAA,CAAAH,GAAA,EAAAC,OAAA;IACA;IACAG,YAAAJ,GAAA,EAAAC,OAAA;MACAC,OAAA,CAAAC,GAAA,CAAAH,GAAA,EAAAC,OAAA;IACA;IACA,MAAAI,cAAA;MACA;QAAAtB,IAAA,EAAAuB;MAAA,UAAA1B,cAAA;MACAsB,OAAA,CAAAC,GAAA,CAAAG,GAAA;MACA,KAAAtB,KAAA,GAAAsB,GAAA,CAAAvB,IAAA;IACA;EACA;EACAwB,QAAA;IACA,KAAAF,aAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}